{
  "name": "monday-standup",
  "version": "0.1.0",
  "description": "Simple web app to faciliate standups in diverse teams",
  "keywords": [
    "standups",
    "diverse",
    "teams",
    "web app",
    "simple",
    "pairing"
  ],
  "bugs": "https://github.com/thoat/monday-standup/issues",
  "repository": "github:thoat/monday-standup",
  "author": "Thoa Ta (http://thriving-thoa.simplesite.com)",
  "private": true,
  "scripts": {
    "start": "node express.js",
    "heroku-postbuild": "yarn build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "client": "react-scripts start",
    "server": "nodemon express.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.3",
    "browserify-fs": "^1.0.0",
    "collections": "^5.1.5",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "http-proxy-middleware": "^0.19.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.4",
    "react": "^16.5.2",
    "react-confirm-alert": "^2.0.6",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.4",
    "shortid": "^2.2.13"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5",
    "yarn": ">= 1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
